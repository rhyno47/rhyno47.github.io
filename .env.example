# Copy this file to .env and fill in the secrets. Do NOT commit your real .env to source control.

# MongoDB connection string (replace <user> and <password> and <dbname>)
MONGO_URI=mongodb+srv://<user>:<password>@cluster0.yourcluster.mongodb.net/<dbname>?retryWrites=true&w=majority

# Optional: OpenAI API key used by AI routes
OPENAI_API_KEY=your_openai_api_key_here

# JWT secret if you implement JSON Web Tokens
JWT_SECRET=replace_with_a_long_random_string
